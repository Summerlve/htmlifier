{
  "name": "htmlifier-electron",
  "version": "1.0.0",
  "description": "htmlifier electron version",
  "main": "index.js",
  "scripts": {
    "test": "electron .",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Summerlve/htmlifier.git"
  },
  "keywords": [
    "convert",
    "scratch3",
    "file",
    "into",
    "executable",
    "program"
  ],
  "author": "Summerlve",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Summerlve/htmlifier/issues"
  },
  "homepage": "https://github.com/Summerlve/htmlifier#readme",
  "devDependencies": {
    "electron": "^9.0.3",
    "electron-builder": "^22.7.0"
  },
  "build": {
    "appId": "edu.qy",
    "directories": {
      "output": "build/"
    },
    "productName": "htmlifier-electron",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "identity": null
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    }
